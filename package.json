{
  "name": "react-conway-life",
  "version": "0.0.1",
  "description": "Conway's Game of Life using React",
  "main": "index.html",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "faucet": "0.0.1",
    "nodemon": "^1.9.2",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "tape": "^4.5.1",
    "watchify": "3.7.0"
  },
  "scripts": {
    "build": "browserify ./src/index.jsx -o ./public/index.js -t babelify",
    "watch": "watchify ./src/index.jsx -o ./public/index.js -t babelify -v",
    "twatch": "nodemon --quiet --delay 1 -e js,jsx -x 'echo \"\n=====================================\n\"; babel-node test/index.js | faucet'",
    "test": "babel-node test/index.js"
  },
  "author": "Brandon Bayer",
  "license": "ISC",
  "private": true
}
